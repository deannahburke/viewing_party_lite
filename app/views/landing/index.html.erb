</h5><h1>Viewing Party Lite</h1>
<%= button_to "Create New User", "/register", method: :get %>

<h3><%= link_to "Log In", "/login" %></h3>

<h3>Existing Users</h3>
<% @users.each_with_index do |user, index| %>
<div id="user-<%= index %>">
  <p><%= user.name %></p>
  <h4><%= link_to "#{user.name}'s Dashboard", "/users/#{user.id}" %></h4>
</div>
<% end %>
